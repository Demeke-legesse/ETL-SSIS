		/// <summary>
        /// This method is called when this script task executes in the control flow.
        /// Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
        /// To open Help, press F1.
        /// </summary>
		public void Main()
		{
            // TODO: Add your code here
            String SendMailFrom = Dts.Variables["User::Var_EmailFROM"].Value.ToString();
            String SendMailTo = Dts.Variables["User::Var_EmailTO"].Value.ToString();
            String SendMailSubject = Dts.Variables["User::Var_EmailSUBJECT"].Value.ToString();
            String SendMailBody = Dts.Variables["User::Var_EmailBODY_Erorr"].Value.ToString() + "\n"+
                 Dts.Variables["System::ErrorDescription"].Value.ToString() + "\n" +
                  Dts.Variables["System::TaskName"].Value.ToString() + "\n" +
                 Dts.Variables["System::EventHandlerStartTime"].Value.ToString() + "\n" +
                  Dts.Variables["System::SourceName"].Value.ToString();
               
                

            try
            {
                MailMessage email = new MailMessage();
                SmtpClient SmtpServer = new SmtpClient("smtp.gmail.com");
                // START
                email.From = new MailAddress(SendMailFrom);
                email.To.Add(SendMailTo);
                email.Subject = SendMailSubject;
                email.Body = SendMailBody;
                //END

                SmtpServer.Port = 587;
                SmtpServer.Credentials = new System.Net.NetworkCredential(SendMailFrom, "elana2015");
                SmtpServer.EnableSsl = true;

                SmtpServer.Send(email);
                MessageBox.Show("Email was Successfully Sent ");
            }
            catch (Exception ex)
            {
                MessageBox.Show(ex.ToString());
            }
            Dts.TaskResult = (int)ScriptResults.Success;
        }


